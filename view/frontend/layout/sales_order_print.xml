<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <referenceBlock name="sales.order.print">
                <referenceBlock name="order_items" template="GreenRivers_DeliveryTime::order/items.phtml">
                    <arguments>
                        <argument name="delivery_time" xsi:type="object">
                            GreenRivers\DeliveryTime\ViewModel\DeliveryTime
                        </argument>
                    </arguments>
                </referenceBlock>
            </referenceBlock>
        </referenceContainer>
    </body>
</page>
