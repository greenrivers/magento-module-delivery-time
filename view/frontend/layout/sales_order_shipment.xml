<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <referenceBlock name="sales.order.shipment">
                <referenceBlock name="shipment_items" template="Unexpected_DeliveryTime::shipment/items.phtml">
                    <arguments>
                        <argument name="delivery_time" xsi:type="object">
                            Unexpected\DeliveryTime\ViewModel\DeliveryTime
                        </argument>
                    </arguments>
                </referenceBlock>
            </referenceBlock>
        </referenceContainer>
    </body>
</page>

